    <model name="iris_demo">
      <include>
	<!--<uri>model://iris_with_ardupilot</uri> -->
	<uri>model://iris_with_ardupilot</uri>
      </include>
      <plugin name="gimbal_controller" filename="libgimbal_controller_plugin.so">
		<tilt_joint>iris_demo::gimbal_small_2d::tilt_joint</tilt_joint>
		<pan_joint>iris_demo::iris_gimbal_mount</pan_joint>
		<target_tilt_topic>/pid/camera/control_effort/y</target_tilt_topic>
		<target_pan_topic>/pid/camera/control_effort/x</target_pan_topic>
		<tilt_pid>
			<p>0.5</p>
			<i>0.1</i>
			<d>0.05</d>
		</tilt_pid>
		<pan_pid>
			<p>0.5</p>
			<i>0.1</i>
			<d>0.05</d>
		</pan_pid>
      </plugin>

    </model>

    <wind>
	    <linear_velocity>
		1 1 0
	    </linear_velocity>
    </wind>
    
    <plugin name="wind" filename="libWindPlugin.so">
  <horizontal>
    <magnitude>
      <time_for_rise>10</time_for_rise>
      <sin>
        <amplitude_percent>1</amplitude_percent>
        <period>20</period>
      </sin>
      <noise type="gaussian">
       <mean>1.0</mean>
       <stddev>0.5</stddev>
      </noise>
    </magnitude>
    <direction>
      <time_for_rise>10</time_for_rise>
      <sin>
        <amplitude>1</amplitude>
        <period>10</period>
      </sin>
      <noise type="gaussian">
       <mean>1</mean>
       <stddev>0.5</stddev>
      </noise>
    </direction>
  </horizontal>
  <vertical>
    <noise type="gaussian">
     <mean>-1</mean>
     <stddev>1</stddev>
    </noise>
  </vertical>
  <!--force_approximation_scaling_factor>0.000001</force_approximation_scaling_factor-->
  <force_approximation_scaling_factor>1e-50</force_approximation_scaling_factor>
</plugin>

    <model name="landing_pad_combo">
      <pose>30 0 0 0 0 0</pose>
      <include>
	<uri>model://landing_pad_combo</uri>
      </include>
      <plugin name="landing_pad_velocity_controller" filename="liblanding_pad_velocity_controller_plugin.so"/>
    </model>

